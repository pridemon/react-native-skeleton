{
  "name": "ReactNativeSkeleton",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --quiet",
    "clean:js": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn install",
    "clean:android": "cd android && ./gradlew clean && ./gradlew cleanBuildCache",
    "clean:ios": "rm -rf ios/Pods/* && rm -rf ~/Library/Caches/CocoaPods/* && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && pod cache clean --all && pod install",
    "postinstall": "yarn jetify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.4",
    "react-native-device-info": "^5.3.0",
    "react-native-iap": "^4.2.2",
    "react-native-libs": "git+https://4579b398f7615b2b10c27229441afc34a4bd42a8:x-oauth-basic@github.com/pridemon/react-native-libs#1.0.0",
    "react-native-localize": "^1.3.1",
    "react-native-rate": "^1.1.10",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/runtime": "7.6.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.18",
    "@types/react-native": "^0.60.22",
    "@types/react-test-renderer": "16.9.0",
    "babel-jest": "24.9.0",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "^5.16.0",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "jetifier": "^1.6.2",
    "lint-staged": "^9.4.3",
    "metro-react-native-babel-preset": "0.56.0",
    "react-native-rename": "^2.4.1",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
